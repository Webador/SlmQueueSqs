{
    "name": "slm/queue-sqs",
    "description": "Zend Framework 2 module that integrates with Amazon SQS queuing system",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "zf2",
        "queue",
        "job",
        "amazon aws",
        "sqs"
    ],
    "homepage": "https://github.com/juriansluiman/SlmQueueSqs",
    "authors": [
        {
            "name": "Jurian Sluiman",
            "email": "jurian@juriansluiman.nl",
            "homepage": "http://juriansluiman.nl"
        },
        {
            "name": "MichaÃ«l Gallego",
            "email": "mic.gallego@gmail.com",
            "homepage": "http://www.michaelgallego.fr"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.2",
        "laminas/laminas-eventmanager": "^3.2.1",
        "laminas/laminas-servicemanager": "^3.3.1",
        "laminas/laminas-stdlib": "^3.2",
        "slm/queue": "^2.0",
        "aws/aws-sdk-php-zf2": "^3.0 || ^4.0"
    },
    "require-dev": {
        "laminas/laminas-config": "^3.3",
        "laminas/laminas-modulemanager": "^2.9",
        "laminas/laminas-view": "^2.11",
        "laminas/laminas-log": "^2.12",
        "laminas/laminas-i18n": "^2.10",
        "laminas/laminas-serializer": "^2.9",
        "laminas/laminas-mvc": "^3.1",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5",
        "php-coveralls/php-coveralls": "^2.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        },
        "zf": {
            "module": "SlmQueueSqs\\Module"
        }
    },
    "autoload": {
        "psr-4": {
            "SlmQueueSqs\\": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SlmQueueSqsTest\\": "tests/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
